{
  "name": "TheSkimm-Trello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint . --color --ext .js,.jsx,.ts,.tsx --cache --cache-location 'node_modules/.cache/.eslintcache/'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --list-different \"./**/*.{json,{j,t}s{x,}}\" --write",
    "prettier:check": "prettier \"./**/*.{json,{j,t}s{x,}}\" -c"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.27",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module-scoped": "^1.0.3",
    "prettier": "^2.4.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-packagejson": "^2.2.13",
    "typescript": "^4.4.3"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "module-scoped"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  }
}
